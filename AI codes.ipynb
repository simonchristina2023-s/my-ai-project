new_donor_scaled = scaler.transform(new_donor)

pred = rf_model.predict(new_donor_scaled)
if 'le_target' in locals():
    pred_label = le_target.inverse_transform(pred)[0]
else:
    pred_label = pred[0]

print("\nPrediction for New Donor:", pred_label)

